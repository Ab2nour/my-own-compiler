<?xml version="1.0" encoding="UTF-8"?>

<tests>
    <titre>Else If</titre>
    
    <test>
        <entree>
            int i
            i = 42
            
            if (i < 11) {
              print(45);
            } else if (i < 40) {
              print(25);
            } else {
              print(42)
            }
        </entree>
        <sortie>
            42
        </sortie>
    </test>
